{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"env": {
		"browser": true
	},
	"globals": {
		"require": true
	},
	"rules": {
		"no-unused-vars": 1
	},
	"plugins": [
		"react"
	],
	"settings": {},
	"parser": "babel-eslint"
}